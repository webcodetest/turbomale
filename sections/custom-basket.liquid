document.addEventListener('DOMContentLoaded', () => {
    document.querySelectorAll('.custom-basket-tabs-content').forEach(tabContent => {
        const products = tabContent.querySelectorAll('.custom-basket-tabs-content-product');
        const loadMoreButton = tabContent.querySelector('.custom-basket-tabs-content-more');
        let visibleItems = 8;

        function getVisibleItems() {
            const screenWidth = window.innerWidth;
            if (screenWidth <= 500) {
                return 4;
            } else if (screenWidth <= 700) {
                return 6;
            } else if (screenWidth <= 1000) {
                return 8;
            } else if (screenWidth <= 1200) {
                return 6;
            } else {
                return 8;
            }
        }

        function updateVisibleProducts() {
            visibleItems = getVisibleItems();

            products.forEach((product, index) => {
                if (index < visibleItems) {
                    product.style.display = 'block';
                } else {
                    product.style.display = 'none';
                }
            });

            if (visibleItems < products.length) {
                loadMoreButton.style.display = 'block';
            } else {
                loadMoreButton.style.display = 'none';
            }
        }

        updateVisibleProducts();

        loadMoreButton.addEventListener('click', function () {
            visibleItems += getVisibleItems();

            products.forEach((product, index) => {
                if (index < visibleItems) {
                    product.style.display = 'block';
                }
            });

            if (visibleItems >= products.length) {
                this.style.display = 'none';
            }
        });

        window.addEventListener('resize', updateVisibleProducts);
    });
});

document.querySelectorAll('.custom-basket-tabs-tab').forEach(tab => {
    tab.addEventListener('click', function () {
        const tabGroup = tab.closest('.custom-basket-tabs');
        const contentWrapper = document.querySelector('.custom-basket-tabs-content-wrapper');
        const targetContent = tab.getAttribute('basket-data-tab');

        tabGroup.querySelectorAll('.custom-basket-tabs-tab').forEach(t => t.classList.remove('active'));
        contentWrapper.querySelectorAll('.custom-basket-tabs-content').forEach(content => content.classList.remove('active'));

        tab.classList.add('active');
        const correspondingContent = contentWrapper.querySelector(`[basket-data-content="${targetContent}"]`);
        if (correspondingContent) {
            correspondingContent.classList.add('active');
        }
    });
});

document.addEventListener('DOMContentLoaded', () => {
    function isMobile() {
        return window.innerWidth <= 1000;
    }

    if (isMobile()) {
        document.querySelectorAll('.custom-basket-tabs-tab').forEach(tab => {
            tab.addEventListener('click', function () {
                const tabGroup = tab.closest('.custom-basket-tabs');
                const contentWrapper = tabGroup.closest('.custom-basket-product-mobile').querySelector('.custom-basket-tabs-content-wrapper');
                const targetContent = tab.getAttribute('basket-data-tab');

                tabGroup.querySelectorAll('.custom-basket-tabs-tab').forEach(t => t.classList.remove('active'));
                contentWrapper.querySelectorAll('.custom-basket-tabs-content').forEach(content => content.classList.remove('active'));

                tab.classList.add('active');
                const correspondingContent = contentWrapper.querySelector(`[basket-data-content="${targetContent}"]`);
                if (correspondingContent) {
                    correspondingContent.classList.add('active');
                }
            });
        });
    }
});


