<script>
document.addEventListener('DOMContentLoaded', () => {
    const basketItemsContainer = document.querySelector('.custom-basket-items');
    const progressBar = document.querySelector('.shipping-progress-bar .progress');
    const shippingMessage = document.querySelector('.shipping-message');
    const freeShippingThreshold = 100;

    function updateProgressBar(cartTotal) {
    const progressPercentage = Math.min((cartTotal / freeShippingThreshold) * 100, 100);
    progressBar.style.width = progressPercentage + '%';

    if (cartTotal >= freeShippingThreshold) {
        shippingMessage.textContent = 'Congratulations! You qualify for free shipping!';
    } else {
        const remaining = freeShippingThreshold - cartTotal;
        shippingMessage.textContent = `You're US$${Math.round(remaining)} away from Free Shipping`;
    }
}

    basketItemsContainer.addEventListener('click', function (event) {
        const button = event.target.closest('.custom-basket-item-trash');
        if (button) {
            const variantId = button.dataset.line;

            fetch('/cart/change.js', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    id: variantId,
                    quantity: 0 
                })
            })
            .then(response => response.json())
            .then(cart => {
                console.log('Cart updated:', cart);
                removeItemFromUI(variantId);
                updateCartSummary(cart);
                updateCartTitle(cart.item_count);
                updateProgressBar(cart.total_price / 100);
            })
            .catch(error => console.error('Error:', error));
        }

        if (event.target.closest('.custom-basket-item-plus, .custom-basket-item-minus')) {
            const button = event.target.closest('.custom-basket-item-plus, .custom-basket-item-minus');
            const input = button.closest('.custom-basket-item-info-bottom-right').querySelector('.custom-basket-item-input');
            const variantId = input.dataset.line;
            let newQuantity = parseInt(input.value, 10);

            if (button.classList.contains('custom-basket-item-minus')) {
                newQuantity = Math.max(1, newQuantity - 1);
            } else if (button.classList.contains('custom-basket-item-plus')) {
                newQuantity = newQuantity + 1;
            }

            fetch('/cart/change.js', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    id: variantId,
                    quantity: newQuantity
                })
            })
            .then(response => response.json())
            .then(cart => {
                console.log('Cart updated:', cart);
                input.value = newQuantity;
                updateCartSummary(cart);
                updateCartTitle(cart.item_count);
                updateProgressBar(cart.total_price / 100);
            })
            .catch(error => console.error('Error:', error));
        }
    });

    fetch('/cart.js')
        .then(response => response.json())
        .then(cart => {
            const cartTotal = cart.total_price / 100;
            updateProgressBar(cartTotal);
        })
        .catch(error => console.error('Error fetching cart data:', error));
});

function removeItemFromUI(variantId) {
    const itemToRemove = document.querySelector(`.custom-basket-item-trash[data-line="${variantId}"]`).closest('.custom-basket-item');
    if (itemToRemove) {
        itemToRemove.remove();
    }
}

function updateCartSummary(cart) {
    const cartTotalPriceElements = document.querySelectorAll('.cart-total-price');
    const totalPrice = cart.total_price / 100;

    cartTotalPriceElements.forEach(element => {
        element.textContent = `$${totalPrice.toFixed(2)}`;
    });
}

function updateCartTitle(itemCount) {
    const cartTitleElement = document.querySelector('h1');
    if (cartTitleElement) {
        cartTitleElement.textContent = `Your cart (${itemCount} item${itemCount !== 1 ? 's' : ''})`;
    }
}

