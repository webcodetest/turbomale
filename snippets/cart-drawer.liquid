{% comment %}
  Renders cart drawer

  Usage:
  {% render 'cart-drawer' %}
{% endcomment %}

{{ 'quantity-popover.css' | asset_url | stylesheet_tag }}
{{ 'component-card.css' | asset_url | stylesheet_tag }}

<script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'quantity-popover.js' | asset_url }}" defer="defer"></script>

<!-- Link Swiper's CSS -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />

<style>
  .drawer {
    visibility: hidden;
  }

  .card-drawer__middle-swiper{
    position: absolute;
    z-index: 100;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    max-width: 720px;
    width: 100%;
  }

  .card-drawer__middle-swiper__head__wrap{
        display: flex;
    align-items: center;
    justify-content: space-between;
  }

  .card-drawer__middle-swiper__heading{
    color: #FFF;
font-family: "Proxima Nova";
font-size: 22px;
font-style: normal;
font-weight: 900;
line-height: normal;
text-transform: uppercase;
  }

  .slide-product__item-prices{
    display: flex;
    align-items: flex-end;
  }

  .slide-product__item-prices__old-price{
    color: #A7A7A7;
font-family: "Proxima Nova";
font-size: 11.397px;
font-style: normal;
font-weight: 400;
line-height: normal;
text-decoration: line-through;
  }

  .slide-product__item-prices__new-price{
    color: #FF0023;
font-family: "Proxima Nova";
font-size: 13.025px;
font-style: normal;
font-weight: 700;
line-height: normal;
  }
  
  .swiper__navigation-arrows{
    display: flex;
    align-items: center;
    gap: 5px;
  }

  .swiper__navigation-arrows button{
    background: none;
    border: none;
    cursor: pointer;
  }

  .progressbar-block-head{
    display: flex;
    align-items: center;
    justify-content: space-between;
  }

  .progressbar-title{
    color: #000;
    font-family: "Proxima Nova";
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
  }
  .progressbar-icon{
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    vertical-align: middle;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    border: 1px solid #8E8E8E;
    position: relative;
    color: #8E8E8E;
    font-family: "Proxima Nova";
    font-size: 12px;
    font-style: normal;
    font-weight: 600;
    line-height: normal;
  }
  
  

  .progressbar-from-to {
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  .low-value {
    color: #000;
    font-family: "Proxima Nova";
    font-size: 16px;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
  }
  .up-value {
    color: #000;
    font-family: "Proxima Nova";
    font-size: 16px;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
  }

  .progressbar-block-footer{
    padding-top: 12px;
  }
  .progressbar-block-footer-info{
    padding: 8px 10px;
    width: 100%;
    border-radius: 6px;
    border: 1px solid #E0E0E0;
    color: #000;
    font-family: "Proxima Nova";
    font-size: 14px;
    font-style: normal;
    font-weight: 600;
    line-height: normal;
    text-align: center;
  }
  .progressbar-green-span{
    color: #60AC22;
    text-decoration-line: underline;
    text-decoration-style: solid;
    text-decoration-skip-ink: none;
    text-decoration-thickness: auto;
    text-underline-offset: auto;
    text-underline-position: from-font;
  }
  
  .cart-item__quantity-wrapper{
    flex-direction: row-reverse;
  }

  .product-drawer-wrapper{
    display: flex;
    padding-top: 16px;
    gap: 22px;
    align-items: center;
  }
  .cart-item__quantity-wrapper{
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  .cart-remove-button .svg-wrapper{
    width: 24px;
    height: 24px;
  }
  .card-drawer-wishlist{
    background: none;
    border: 0;
    min-width: 3.5rem;
    min-height: 3.5rem;
  }
  .product-drawer-item-link a{
    color: #000;
    text-decoration: none;
    font-family: "Proxima Nova";
    font-size: 18px;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
  }
  .product-option{
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #FF0023;
    font-family: "Proxima Nova";
    font-size: 16px !important;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
    word-break: normal;
    opacity: 1;
  }

  .quantity:after, .quantity:before{
    content:'';
    display: none !important;
  }
  quantity-popover .quantity{
    width: 111px;
    height: 36px;
    box-shadow: 0px 0px 18.6px 0px rgba(0, 0, 0, 0.07);
    border-radius: 12px;
    overflow: hidden;
    max-width: 100%;
  }
  .quantity__button .svg-wrapper{
    color: #60AC22;
  }
  .cart-drawer .drawer__cart-items-wrapper{
    padding: 0 5px;
  }
  .quantity__input{
    color: #000;
    padding: 0;
    text-align: center;
    font-family: "Proxima Nova";
    font-size: 20px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
  }
  .drawer__footer{
    padding: 6px 0 0 0;
  }
  .cart-drawer__footer > * + *{
    margin-top: 6px;
  }
  .cart-drawer__footer .progressbar-block-footer-info{
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 6px;
    text-align: center;
    margin: 12px 0 13px 0;
  }
  .totals > h2{
    color: #000;
    font-family: "Proxima Nova";
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
  }
  .totals__total{
    margin-top: 0;
  }
  .totals__total-value{
    color: #000;
    font-family: "Proxima Nova";
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
  }

  .swiper {
      width: 100%;
      height: 100%;
    }

    .swiper-slide {
      text-align: center;
      font-size: 18px;
      background: #fff;
      display: flex;
      justify-content: center;
      align-items: center;
    }

    .swiper-slide img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .card-drawer-bottom-swiper .swiper-button-next,
    .card-drawer-bottom-swiper .swiper-button-prev{
      display: block !important;
    }

    .low-btn{
      background: #60AC22;
      border-radius: 12px;
      display: flex;
      justify-content: center;
      align-items: center;
      padding: 10px;
      border: none;
      margin: 5px 16px 0 0;
    }
    .product-item-wrap{
      display: flex;
      gap: 12px;
      align-items: center;
    }
    .product-item-second-right-part{
      display: flex;
      align-items: center;
      justify-content: space-between;
    }
    .product-item-second-right-part p {
      color: #ED1731;
      font-family: "Proxima Nova";
      font-size: 16px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
    .product-item-second-right-part .grey-line-throw-price{
      font-size: 14px;
      color: #A7A7A7;
      text-decoration: line-through;
      font-weight: 400;
    }
    .product-item-right-part h2{
      color: #000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
    .also-like-head{
      display: flex;
      align-items: center;
      justify-content: space-between;
      color: #000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
    .cart-drawer .cart__checkout-button{
      border-radius: 12px;
      background: #40AE00;
      color: #FFF;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
      text-transform: uppercase;
    }

    .progressbar-block-head-popup.unvisible{
      opacity: 0;
      pointer-events: none;
    }

    .progressbar-block-head-popup {
      border-radius: 12px;
      width: 340px;
      border: 1px solid #E0E0E0;
      padding: 20px;
      position: absolute;
      background: #fff;
      top: 30px;
      right: -1px;
      z-index: 5;
      opacity: 1;
      pointer-events: all;
      transition: all 0.3s ease;
      opacity: 0;
      pointer-events: none;
      text-align: left;
    }
    .popup-opener-one:hover .progressbar-block-head-popup{
      opacity: 1;
      pointer-events: all;
    }
    .progressbar-block-head-popup-wrap {
    }
    .head-popup-title {
      color: #000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
    .head-popup-title:not(:first-child){
      padding-top: 15px;
    }
    .head-popup-content {
      color: #000;
      padding-top: 9px;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 400;
      line-height: normal;
    }
    .head-popup-content.head-popup-content-few-items{
      display: flex;
      align-items: center;
      justify-content: space-between;
    }
    .head-popup-content.head-popup-content-border{
      border-bottom: 1px solid #D9D9D9;
      padding-bottom: 15px;
    }
    .head-popup-green {
      color: #40AE00;
      text-align: right;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }

    .item-variant{
      padding-top: 2px;
      color: #000;
      font-family: "Proxima Nova";
      font-size: 16px;
      font-style: normal;
      font-weight: 400;
      line-height: normal;
    }
    
    .cart-item__discounted-prices{
      display: flex;
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding-bottom: 20px;
    }
    .cart-item__discounted-price{
      color: #A7A7A7;
      font-family: "Proxima Nova";
      font-size: 14px;
      font-style: normal;
      font-weight: 400;
      line-height: normal;
      text-decoration: line-through;
    }
    .item-popup-two {
      padding: 20px;
      z-index: 10;
      border-radius: 12px;
      border: 1px solid #E0E0E0;
      background: #FFF;
      position: absolute;
      right: 0;
      transition: all 0.3s ease;
      opacity: 0;
      pointer-events: none;
    }
    .item-popup-two-last-price-wrap {
      display: flex;
      align-items: center;
      flex-direction: row;
      width: 220px;
      justify-content: space-between;
    }
    .item-popup-two-last-price-text {
      color: #000;
      color: #000000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 400;
      line-height: normal;
    }
    .item-popup-two-last-price-value {
      color: #000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 400;
      line-height: normal;
    }
    .sale-green-item {
      color: #40AE00;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
      padding: 9px 0 15px 0;
    }
    .sale-bold-item{
      padding-top: 15px;
      color: #000;
      font-family: "Proxima Nova";
      font-size: 18px;
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
    .item-popup-two-last-price-wrap:nth-child(2){
      border-bottom: 1px solid #D9D9D9;
    }

    .cart-item__discounted-prices .progressbar-icon:hover .item-popup-two{
      opacity: 1;
      pointer-events: all;
    }

    .swiper-button-next, .swiper-button-prev{
      display: flex !important; 
    }

    .slider-container{
      display: flex;
      overflow: hidden;
    }

    .product-item{
      min-width: 100%;
      flex-shrink: 1;
      margin-right: 16px;
      border: 1px solid #E0E0E0;
    }

    .prev-arrow, .next-arrow{
      background: none;
      border: none;
      cursor: pointer;
    }

</style>
<div class="cart-drawer-wrapper">
<cart-drawer class="drawer{% if cart == empty %} is-empty{% endif %}">
<div class="card-drawer__middle-swiper">

      <div class="card-drawer__middle-swiper__head__wrap">
        <h3 class="card-drawer__middle-swiper__heading">people also bought</h3>
        <div class="swiper__navigation-arrows">
          <button type="button" class="swiper__left-arr">
            <svg xmlns="http://www.w3.org/2000/svg" width="59" height="59" viewBox="0 0 59 59" fill="none">
              <path fill-rule="evenodd" clip-rule="evenodd" d="M25.5097 29.012L35 20.9588L33.7382 19L22 29.0487L33.7099 39L34.9717 37.0412L25.5097 29.012Z" fill="#60AC22" style="fill:#60AC22;fill:color(display-p3 0.3768 0.6749 0.1322);fill-opacity:1;"/>
            </svg>
          </button>
          <button type="button" class="swiper__right-arr">
            <svg xmlns="http://www.w3.org/2000/svg" width="59" height="59" viewBox="0 0 59 59" fill="none">
              <path fill-rule="evenodd" clip-rule="evenodd" d="M33.4903 29.012L24 20.9588L25.2618 19L37 29.0487L25.2901 39L24.0283 37.0412L33.4903 29.012Z" fill="#60AC22" style="fill:#60AC22;fill:color(display-p3 0.3768 0.6749 0.1322);fill-opacity:1;"/>
            </svg>
          </button>
        </div>
      </div>
  <div class="swiper card-drawer__middle-swiper-item">
    <div class="swiper-wrapper">
      {% if collections['Extenders'] and collections['Extenders'].products.size > 0 %}
        {% for product in collections['Extenders'].products %}
          <div class="swiper-slide">
            {% comment %}
            <img src="{{ product.featured_image | img_url: 'medium' }}" alt="{{ product.title }}">              
            {% endcomment %}
            <div class="slide-product__wrapper">
              <div class="slide-product__wishlist-icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="21" height="18" viewBox="0 0 21 18" fill="none">
                  <path d="M5.79597 0.785324C3.31564 0.968488 1.25505 2.84973 0.869645 5.2919C0.785695 5.8185 0.812407 6.8755 0.923068 7.34867C1.17492 8.42476 1.64809 9.29478 2.48377 10.2182C3.36524 11.1913 5.76926 13.3473 8.58158 15.6864C9.8828 16.7663 10.0545 16.9037 10.2033 16.98C10.3941 17.0792 10.7872 17.0716 10.9856 16.9686C11.2146 16.8541 14.3283 14.2478 15.8509 12.9008C17.3505 11.5729 18.6136 10.3518 19.0219 9.83664C19.6477 9.04675 20.0293 8.27212 20.2468 7.34867C20.3575 6.86405 20.3842 5.80705 20.2964 5.27664C19.911 2.95276 18.0794 1.16691 15.7173 0.819667C15.3014 0.758613 14.4123 0.766245 14.0192 0.834931C13.237 0.972303 12.4585 1.28902 11.806 1.73548C11.619 1.86522 11.268 2.15905 11.0238 2.39182L10.5849 2.81157L10.1461 2.39182C9.90188 2.15905 9.55464 1.86904 9.36766 1.7393C8.30302 1.01046 7.10483 0.689927 5.79597 0.785324ZM7.24602 2.66656C7.63524 2.78486 8.09696 3.00618 8.38697 3.21224C8.49382 3.28855 8.90975 3.67396 9.3066 4.067C9.70727 4.46004 10.0889 4.80728 10.1614 4.84544C10.2339 4.87979 10.3903 4.92176 10.5162 4.92939C10.9131 4.96755 11.0047 4.9065 11.8442 4.08608C12.2525 3.68159 12.676 3.28855 12.7829 3.21224C13.0653 3.00999 13.527 2.78486 13.9238 2.66656C14.2444 2.56735 14.3131 2.55972 14.9351 2.55972C15.5532 2.55972 15.6296 2.56735 15.9386 2.66275C17.1368 3.03289 18.0298 3.88002 18.4037 5.00189C18.8082 6.21917 18.5563 7.52802 17.7092 8.64226C17.3658 9.08872 16.1447 10.2717 14.7443 11.5042C13.2179 12.8512 10.6536 15.0148 10.5849 15.0148C10.5391 15.0148 9.16923 13.8739 7.76116 12.6642C5.21214 10.4739 3.75065 9.08491 3.28893 8.40186C1.72822 6.10469 2.83865 3.19697 5.5136 2.59406C5.68531 2.5559 5.92571 2.54064 6.3302 2.55209C6.81482 2.56353 6.95601 2.5788 7.24602 2.66656Z" fill="#C7C7C7" style="fill:#C7C7C7;fill:color(display-p3 0.7822 0.7822 0.7822);fill-opacity:1;"/>
                </svg>
              </div>
              <div class="slide-product__item-img">
                <img src="{{ product.featured_image | img_url: 'medium' }}" alt="{{ product.title }}">
              </div>
              <div class="slide-product__item-review">
                <div class='jdgm-widget jdgm-preview-badge'>
                  {{ product.metafields.judgeme.badge }}
                </div>
              </div>
              <a href="{{ product.url }}" class="slide-product__item-title">
                {{ product.title }}
              </a>
              <div class="slide-product__item-prices">
                <div class="slide-product__item-prices__new-price">{{ product.price | money_with_currency }}</div>
                <div class="slide-product__item-prices__old-price">{{ product.compare_at_price | money_with_currency }}</div>
                <a href="{{ product.url }}" class="slide-product__item-prices__add-to-card-btn">
