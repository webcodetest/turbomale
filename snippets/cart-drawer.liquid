        function updateDateRange() {
          const currentDate = new Date();
          const futureDate = new Date(currentDate);
          futureDate.setDate(currentDate.getDate() + 3);

          const formattedCurrentDate = formatDate(currentDate);
          const formattedFutureDate = formatDate(futureDate);

          const dateRangeElement = document.getElementById('days-range');
          if (dateRangeElement) {
            dateRangeElement.textContent = `${formattedCurrentDate} - ${formattedFutureDate}`;
          }
        }

        updateDateRange();

        const dateObserver = new MutationObserver((mutationsList) => {
          for (const mutation of mutationsList) {
            if (mutation.type === 'childList' || mutation.type === 'attributes') {
              if (document.getElementById('days-range')) {
                updateDateRange();
                break;
              }
            }
          }
        });

        dateObserver.observe(document.body, {
          childList: true,
          subtree: true,
          attributes: true,
        });

